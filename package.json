{
  "name": "react-showcase",
  "description": "react-showcase application",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "html-minimizer-webpack-plugin": "4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "5.0.1",
    "react-query": "3.39.3",
    "react-redux": "9.0.4",
    "react-router-dom": "6.21.2",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "webpack serve",
    "build": "CI=false && webpack --mode=production --node-env=production",
    "lint": "eslint src --ext js",
    "test": "cross-env BABEL_ENV=test jest",
    "watch": "webpack --watch",
    "coverage": "cross-env BABEL_ENV=test --coverage",
    "full-production": "npm i && npm run build",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "serve": "webpack serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@webpack-cli/generators": "^3.0.7",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
